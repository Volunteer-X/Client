{
  "extends": [
    "@react-native/typescript-config/tsconfig.json"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
      "@theme/*": [
        "src/theme/*"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@ts-types/*": [
        "src/types/*"
      ]
    },
    "resolveJsonModule": true,
    // "typeRoots": [
    //   "./src/types"
    // ]
  },
}